{
	"name": "ipad54/bedrockeditionclient",
        "description": "Implementation of Minecraft: Bedrock Edition client based on fork PocketMine-MP",
        "type": "project",
        "homepage": "https://github.com/Faer12312333/BedrockEditionClient-NG",
        "license": "GPL-3.0",
	"require": {
		"php": "^8.1",
		"ext-openssl": "*",
		"nethergamesmc/pocketmine-mp": "dev-stable"
	},
	"require-dev": {
		"phpstan/phpstan": "1.7.8",
		"phpstan/phpstan-phpunit": "^1.1.0",
		"phpstan/phpstan-strict-rules": "^1.2.0",
		"phpunit/phpunit": "^9.2"
	},
	"autoload": {
		"psr-4": {
			"": [
				"src"
			]
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:NetherGamesMC/PocketMine-MP.git"
		}
	],
	"minimum-stability": "dev"
}
